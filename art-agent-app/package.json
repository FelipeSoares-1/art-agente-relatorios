{
  "name": "art-agent-app",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest --watch",
    "postinstall": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "start:cron": "tsx src/scripts/start-cron.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "csv-parse": "^6.1.0",
    "framer-motion": "^12.23.24",
    "html-to-text": "^9.0.5",
    "lucide-react": "^0.554.0",
    "next": "16.0.3",
    "node-cron": "^4.2.1",
    "puppeteer": "^24.30.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
